digraph G {
    graph [
        nodesep=0.5;
        rankdir="LR";
        cencentrate=true;
        splines="spline";
        fontname="Helvetica";
        pad="0.2,0.2",
        label="{{lbl}}",
        {{gs}}
    ];
    
    node [shape=plain, fontname="Helvetica"];
    edge [
        dir=both,
        fontsize=12,
        arrowsize=0.9,
        penwidth=1.0,
        labelangle=32,
        labeldistance=1.8,
        fontname="Helvetica"
    ];
    {{!base}}

}